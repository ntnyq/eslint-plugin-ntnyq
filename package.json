{
  "name": "eslint-plugin-ntnyq",
  "type": "module",
  "version": "0.9.2",
  "packageManager": "pnpm@10.2.1",
  "description": "An opinionated ESLint plugin.",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "ntnyq"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://eslint-plugin.ntnyq.com",
  "repository": "ntnyq/eslint-plugin-ntnyq",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "coverage": "vitest --coverage",
    "deploy": "run-s build docs:build",
    "dev": "tsup --watch src",
    "docs:build": "pnpm -C docs run build",
    "docs:dev": "pnpm -C docs run dev",
    "lint": "eslint .",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build",
    "release": "run-s release:check release:publish",
    "release:check": "run-s lint typecheck test",
    "release:publish": "bumpp && pnpm publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "eslint": "^9.5.0"
  },
  "dependencies": {
    "@typescript-eslint/parser": "^8.23.0",
    "@typescript-eslint/utils": "^8.23.0"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^3.12.3",
    "@ntnyq/prettier-config": "^1.22.1",
    "@types/node": "^22.13.1",
    "@vitest/coverage-v8": "^3.0.5",
    "bumpp": "^10.0.2",
    "eslint": "^9.19.0",
    "eslint-plugin-eslint-plugin": "^6.4.0",
    "eslint-vitest-rule-tester": "^1.1.0",
    "husky": "^9.1.7",
    "nano-staged": "^0.8.0",
    "npm-run-all2": "^7.0.2",
    "prettier": "^3.4.2",
    "tsup": "^8.3.6",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "catalog:",
    "vitest": "^3.0.5"
  },
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=21.1.0"
  },
  "nano-staged": {
    "*.{js,ts,cjs,mjs,vue,md,yml,yaml,json}": "eslint --fix"
  }
}
