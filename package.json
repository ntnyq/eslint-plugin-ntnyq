{
  "name": "eslint-plugin-ntnyq",
  "version": "0.1.0",
  "packageManager": "pnpm@8.6.12",
  "description": "An opinionated ESLint plugin.",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "ntnyq"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://eslint-plugin.ntnyq.com",
  "repository": "ntnyq/eslint-plugin-ntnyq",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.mjs"
    },
    "./*": "./*"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "typesVersions": {
    "*": {
      "*": [
        "./dist/*",
        "./dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "scripts": {
    "build": "tsup",
    "coverage": "vitest --coverage",
    "dev": "DEV=1 tsup",
    "docs:build": "pnpm -C docs run build",
    "docs:dev": "pnpm -C docs run dev",
    "lint": "eslint .",
    "prepare": "husky install",
    "publish": "pnpm build && changeset publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@typescript-eslint/utils": "^6.3.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.4.8",
    "@changesets/cli": "^2.26.2",
    "@ntnyq/eslint-config": "^1.12.2",
    "@ntnyq/prettier-config": "^1.12.2",
    "@types/eslint": "^8.44.2",
    "@types/node": "^20.4.9",
    "@typescript-eslint/parser": "^6.3.0",
    "@vitest/coverage-v8": "^0.34.1",
    "eslint": "^8.46.0",
    "husky": "^8.0.3",
    "nano-staged": "^0.8.0",
    "pnpm": "^8.6.12",
    "prettier": "^3.0.1",
    "tsup": "^7.2.0",
    "tsx": "^3.12.7",
    "typescript": "^5.1.6",
    "vite-tsconfig-paths": "^4.2.0",
    "vitest": "^0.34.1"
  },
  "engines": {
    "node": ">=16.14.0"
  },
  "prettier": "@ntnyq/prettier-config",
  "nano-staged": {
    "*.{js,ts,md,yml,yaml,json}": "eslint --fix"
  }
}
