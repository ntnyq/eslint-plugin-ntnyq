{
  "name": "eslint-plugin-ntnyq",
  "type": "module",
  "version": "0.11.0",
  "packageManager": "pnpm@10.10.0",
  "description": "An opinionated ESLint plugin.",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "ntnyq"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://eslint-plugin.ntnyq.com",
  "repository": "ntnyq/eslint-plugin-ntnyq",
  "bugs": {
    "url": "https://github.com/ntnyq/eslint-plugin-ntnyq/issues"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "coverage": "vitest --coverage",
    "deploy": "run-s build docs:build",
    "dev": "unbuild --watch",
    "docs:build": "pnpm -C docs run build",
    "docs:dev": "pnpm -C docs run dev",
    "lint": "eslint",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build",
    "release": "run-s release:check release:version release:publish",
    "release:check": "run-s lint typecheck test",
    "release:publish": "pnpm publish",
    "release:version": "bumpp",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "eslint": "^9.5.0"
  },
  "dependencies": {
    "@ntnyq/utils": "catalog:",
    "@typescript-eslint/parser": "catalog:",
    "@typescript-eslint/utils": "catalog:"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "catalog:",
    "@ntnyq/prettier-config": "catalog:",
    "@types/node": "catalog:",
    "@vitest/coverage-v8": "catalog:",
    "bumpp": "catalog:",
    "eslint": "catalog:",
    "eslint-plugin-eslint-plugin": "catalog:",
    "eslint-vitest-rule-tester": "catalog:",
    "husky": "catalog:",
    "nano-staged": "catalog:",
    "npm-run-all2": "catalog:",
    "prettier": "catalog:",
    "typescript": "catalog:",
    "typescript-eslint": "catalog:",
    "unbuild": "catalog:",
    "vitest": "catalog:"
  },
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=21.1.0"
  },
  "nano-staged": {
    "*.{js,ts,cjs,mjs,vue,md,yml,yaml,json}": "eslint --fix",
    "*.{css,scss,html}": "prettier -uw"
  }
}
