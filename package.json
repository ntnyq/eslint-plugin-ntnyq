{
  "name": "eslint-plugin-ntnyq",
  "type": "module",
  "version": "0.5.0",
  "packageManager": "pnpm@9.10.0",
  "description": "An opinionated ESLint plugin.",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "ntnyq"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://eslint-plugin.ntnyq.com",
  "repository": "ntnyq/eslint-plugin-ntnyq",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "coverage": "vitest --coverage",
    "dev": "tsup --watch src",
    "docs:build": "pnpm -C docs run build",
    "docs:dev": "pnpm -C docs run dev",
    "lint": "eslint .",
    "prepare": "husky",
    "publish": "pnpm build && changeset publish",
    "test": "vitest",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "eslint": "^9.0.0"
  },
  "dependencies": {
    "@typescript-eslint/utils": "^8.6.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.0",
    "@changesets/cli": "^2.27.8",
    "@ntnyq/eslint-config": "^3.0.0-beta.13",
    "@ntnyq/prettier-config": "^1.21.3",
    "@types/node": "^22.5.5",
    "@typescript-eslint/parser": "^8.6.0",
    "@vitest/coverage-v8": "^2.1.1",
    "eslint": "^9.10.0",
    "eslint-vitest-rule-tester": "^0.6.1",
    "husky": "^9.1.6",
    "nano-staged": "^0.8.0",
    "pnpm": "^9.10.0",
    "prettier": "^3.3.3",
    "tsup": "^8.3.0",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2",
    "typescript-eslint": "^8.6.0",
    "vitest": "^2.1.1"
  },
  "engines": {
    "node": "^18.18.0 || ^20.9.0 || >=21.1.0"
  },
  "prettier": "@ntnyq/prettier-config",
  "nano-staged": {
    "*.{js,ts,cjs,mjs,md,yml,yaml,json}": "eslint --fix"
  }
}
