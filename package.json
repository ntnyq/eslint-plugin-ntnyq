{
  "name": "eslint-plugin-ntnyq",
  "type": "module",
  "version": "0.12.0",
  "packageManager": "pnpm@10.28.2",
  "description": "An opinionated ESLint plugin.",
  "keywords": [
    "eslint",
    "eslint-plugin",
    "ntnyq"
  ],
  "license": "MIT",
  "author": {
    "name": "ntnyq",
    "email": "ntnyq13@gmail.com"
  },
  "homepage": "https://eslint-plugin.ntnyq.com",
  "repository": "ntnyq/eslint-plugin-ntnyq",
  "bugs": {
    "url": "https://github.com/ntnyq/eslint-plugin-ntnyq/issues"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsdown",
    "deploy": "run-s build docs:build",
    "dev": "tsdown --watch",
    "docs:build": "pnpm -C docs run build",
    "docs:dev": "pnpm -C docs run dev",
    "format": "oxfmt",
    "format:check": "oxfmt --check",
    "lint": "eslint",
    "prepare": "husky",
    "prepublishOnly": "pnpm run build",
    "release": "run-s release:check release:version",
    "release:check": "run-s format:check lint typecheck test",
    "release:version": "bumpp",
    "test": "vitest",
    "typecheck": "tsgo --noEmit"
  },
  "peerDependencies": {
    "eslint": "^9.5.0"
  },
  "dependencies": {
    "@ntnyq/utils": "^0.11.0",
    "@typescript-eslint/parser": "^8.54.0",
    "@typescript-eslint/utils": "^8.54.0"
  },
  "devDependencies": {
    "@ntnyq/eslint-config": "^6.0.0-beta.7",
    "@types/node": "^25.2.1",
    "@typescript/native-preview": "^7.0.0-dev.20260205.1",
    "bumpp": "^10.4.1",
    "eslint": "^9.39.2",
    "eslint-plugin-eslint-plugin": "^7.3.0",
    "eslint-vitest-rule-tester": "^3.0.1",
    "husky": "^9.1.7",
    "nano-staged": "^0.9.0",
    "npm-run-all2": "^8.0.4",
    "oxfmt": "^0.28.0",
    "tinyglobby": "^0.2.15",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  },
  "nano-staged": {
    "*.{js,ts,mjs,tsx,vue,md,yml,yaml,json}": "eslint --fix",
    "*": "oxfmt --no-error-on-unmatched-pattern"
  }
}
